{
  "source": {
    "include": ["./src/", "./index.js"],
    "includePattern": "\\.(js|jsx)$",
    "exclude": ["node_modules/", "coverage/", "tests/"]
  },
  "opts": {
    "destination": "./docs/api/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}